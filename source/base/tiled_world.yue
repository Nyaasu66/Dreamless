import "bump"
import "sti"
import "deep"
import "camera"
import "vector"

class TiledWorld
    new: (path_to_map) =>
        @camera = camera 0, 0
        @world = bump 64
        @map = sti path_to_map, {"bump"}
        @map::bump_init @world
    update: (dt) =>
        @map::update dt
    draw: =>
        @map::draw @camera::cameraCoords 0, 0
